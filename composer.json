{
    "name": "hyperlife1119/onchat",
    "description": "A simple instant messaging website backend build with ThinkPHP6.",
    "type": "project",
    "keywords": [
        "onchat",
        "thinkphp"
    ],
    "homepage": "https://chat.hypergo.net/",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "HyperLife1119",
            "email": "hyperlife1119@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "topthink/framework": "6.0.11",
        "topthink/think-orm": "2.0.50",
        "topthink/think-captcha": "3.0.3",
        "topthink/think-swoole": "4.0.6",
        "topthink/think-queue": "3.0.6",
        "aliyuncs/oss-sdk-php": "2.4.3",
        "yzalis/identicon": "2.0.0",
        "ezyang/htmlpurifier": "4.14.0",
        "phpmailer/phpmailer": "6.5.3",
        "php-ffmpeg/php-ffmpeg": "0.19.0",
        "firebase/php-jwt": "5.5.1"
    },
    "require-dev": {
        "symfony/var-dumper": "5.4.2",
        "topthink/think-trace": "1.4"
    },
    "autoload": {
        "psr-4": {
            "app\\": "app"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php think service:discover",
            "@php think vendor:publish"
        ]
    }
}